<div id = container>
<h1>Listing <%= pluralize(@dojos.count, "Dojo") %></h1>

<table border="1">
	<thead>
		<th>Coding Dojo Branch</th>
		<th>Street</th>
		<th>City</th>
        <th>State</th>
        <th> Actions</th>
	</thead>
	<tbody>
		<% @dojos.each do |dojo| %>
		<tr>
			<td><%= dojo.branch %></td>
			<td><%= dojo.street %></td>
			<td><%= dojo.city %></td>
            <td><%= dojo.state %></td>
            <td><a href="dojos/<%= dojo.id %>">Show</a> | 
                <a href="dojos/<%= dojo.id %>/edit">Edit</a> |
                <a data-confirm="You Sure?" rel="nofollow" data-method="delete" href="/dojos/<%= dojo.id %>">Remove</a>
            </td>
		</tr>
		<% end %>
	</tbody>
</table>
<a href="dojos/new">Create New Dojo </a>
</div>
